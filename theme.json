{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        }
      }
    },
    "custom": {
      "patterns": {
        "enabled": true
      }
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "customFontSize": true,
      "lineHeight": true,
      "fontWeight": true,
      "letterSpacing": true
    },
    "color": {
      "custom": true,
      "customGradient": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "primary",
          "color": "#1a3d0a",
          "name": "Primary"
        },
        {
          "slug": "accent",
          "color": "#f97316",
          "name": "Accent"
        },
        {
          "slug": "bg-secondary",
          "color": "#f8f8f8",
          "name": "Background Secondary"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        }
      ]
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}